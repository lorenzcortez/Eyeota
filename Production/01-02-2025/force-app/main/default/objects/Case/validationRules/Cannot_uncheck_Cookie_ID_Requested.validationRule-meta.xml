<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_uncheck_Cookie_ID_Requested</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISCHANGED(Cookie_ID_Requested__c ), PRIORVALUE(Cookie_ID_Requested__c ) = true)</errorConditionFormula>
    <errorMessage>You cannot uncheck the Cookie ID Requested field.</errorMessage>
</ValidationRule>
