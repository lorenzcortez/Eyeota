<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LinkedIn_Profile_or_Email_check</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISBLANK(Email),  ISBLANK(LinkedIn_Profile__c))</errorConditionFormula>
    <errorMessage>Please fill in either the Email or LinkedIn Profile field.</errorMessage>
</ValidationRule>
