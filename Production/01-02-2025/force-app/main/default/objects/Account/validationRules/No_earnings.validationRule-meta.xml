<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>No_earnings</fullName>
    <active>false</active>
    <errorConditionFormula>AND(No_Earnings__c=TRUE,
OR(
OR(Data_Supplier_Earnings_Uploaded__c=TRUE,
Data_Supplier_Earnings_Report_Reviewed__c=TRUE),
Data_Supplier_Earnings_Report_Sent__c=TRUE))</errorConditionFormula>
    <errorMessage>This Supplier has been marked as &apos;No Earnings&apos;.</errorMessage>
</ValidationRule>
